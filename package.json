{
  "name": "wot-design-mini",
  "version": "1.1.0",
  "description": "A ui component library for mini program",
  "scripts": {
    "dev": "npm run build:icon && cross-env EFF_ABSOLUTE_PATHS=true gulp dev --env development --gulpfile build/gulpfile.js",
    "dev:wx": "npm run build:icon && cross-env EFF_ABSOLUTE_PATHS=true gulp devwx --env development --gulpfile build/gulpfile.js",
    "dev:docs": "webpack-dev-server --progress --config build/webpack.docs.conf.js",
    "build": "gulp build --env production --gulpfile build/gulpfile.js",
    "build:wx": "gulp buildwx --env production --gulpfile build/gulpfile.js",
    "build:docs": "rimraf docs/dist && cross-env NODE_ENV=production webpack --progress --config build/webpack.docs.conf.js",
    "build:icon": "node build/iconInit.js",
    "fix": "cross-env EFF_ABSOLUTE_PATHS=true eslint --fix --format friendly --ext .js,.json packages",
    "tag": "node build/build-tag.js"
  },
  "keywords": [
    "wot",
    "design",
    "wot-design",
    "wot-design-mini",
    "jd",
    "wechat"
  ],
  "license": "ISC",
