<wd-picker-view
  custom-class="custom-class"
  id="{{pickerId}}"
  value="{{pickerValue}}"
  columns="{{columns}}"
  columns-height="{{columnsHeight}}"
  cancel-button-text="{{cancelButtonText}}"
  confirm-button-text="{{confirmButtonText}}"
  loading="{{loading}}"
  bind:change="onChange"
></wd-picker-view>